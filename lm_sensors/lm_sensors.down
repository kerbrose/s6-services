#!/usr/bin/execlineb -P
foreground { 
	envfile -I /etc/sysconfig/lm_sensors
	redirfd -w 2 /dev/null importas -sCiu BUS_MODULES BUS_MODULES
	exec modprobe -qabr ${BUS_MODULES}
}
foreground {
	envfile -I /etc/sysconfig/lm_sensors
	redirfd -w 2 /dev/null importas -sCiu HWMON_MODULES HWMON_MODULES
	exec modprobe -qabr ${HWMON_MODULES}
}
