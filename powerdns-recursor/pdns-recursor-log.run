#!/usr/bin/execlineb -P
foreground { if -n -t { test -d /var/log/pdns-recursor } install -d -m 0755 -o s6log -g s6log /var/log/pdns-recursor }
envfile /etc/s6/sv/pdns-recursor-log/conf
importas -sCiu DIRECTIVES DIRECTIVES
s6-setuidgid s6log exec -c s6-log -d3 -b -- ${DIRECTIVES} /var/log/pdns-recursor
