#!/usr/bin/execlineb -P
foreground { if -n -t { test -d /var/log/wpa_supplicant } install -d -m 0755 -o s6log -g s6log /var/log/wpa_supplicant }
envfile /etc/s6/sv/wpa_supplicant-log/conf
importas -sCiu DIRECTIVES DIRECTIVES
s6-setuidgid s6log exec -c s6-log -d3 -b -- ${DIRECTIVES} /var/log/wpa_supplicant
