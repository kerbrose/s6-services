#!/usr/bin/execlineb -P
foreground { if -n -t { test -d /var/log/doh-client } install -d -m 0755 -o s6log -g s6log /var/log/doh-client }
envfile /etc/s6/config/dns-over-https.conf
importas -sCiu DOH_CLIENT_DIRECTIVES DOH_CLIENT_DIRECTIVES
s6-setuidgid s6log exec -c s6-log -d3 -b -- ${DOH_CLIENT_DIRECTIVES} /var/log/doh-client
