#!/usr/bin/execlineb -P
foreground { if -n -t { test -d /var/log/squid } install -d -m 0755 -o s6log -g s6log /var/log/squid }
foreground { chmod -R 755 /var/log/squid }
envfile /etc/s6/sv/squid-log/conf
importas -sCiu DIRECTIVES DIRECTIVES
s6-setuidgid s6log exec -c s6-log -d3 -b -- ${DIRECTIVES} /var/log/squid
