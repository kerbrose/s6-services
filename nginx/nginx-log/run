#!/usr/bin/execlineb -P
foreground { if -n -t { test -d /var/log/nginx } install -d -m 0755 -o s6log -g s6log /var/log/nginx }
foreground { chmod 755 -R /var/log/nginx }
foreground { chmod -R 755 /var/log/nginx }
envfile /etc/s6/config/nginx.conf
importas -sCiu DIRECTIVES DIRECTIVES
s6-setuidgid s6log exec -c s6-log -d3 -b -- ${DIRECTIVES} /var/log/nginx
