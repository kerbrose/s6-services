#!/usr/bin/execlineb -P
foreground { if -n -t { test -d /var/log/deluge-web } install -d -m 0755 -o s6log -g s6log /var/log/deluge-web }
envfile /etc/s6/config/deluge.conf
importas -sCiu DELUGE_WEB_DIRECTIVES DELUGE_WEB_DIRECTIVES
s6-setuidgid s6log exec -c s6-log -d3 -b -- ${DELUGE_WEB_DIRECTIVES} /var/log/deluge-web
