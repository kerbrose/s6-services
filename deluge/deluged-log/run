#!/usr/bin/execlineb -P
foreground { if -n -t { test -d /var/log/deluged } install -d -m 0755 -o s6log -g s6log /var/log/deluged }
envfile /etc/s6/config/deluge.conf
importas -sCiu DELUGED_DIRECTIVES DELUGED_DIRECTIVES
s6-setuidgid s6log exec -c s6-log -d3 -b -- ${DELUGED_DIRECTIVES} /var/log/deluged
