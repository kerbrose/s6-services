#!/usr/bin/execlineb -P
foreground { if -n -t { test -d /var/log/lightdm } install -d -m 0755 -o s6log -g s6log /var/log/lightdm }
foreground { chown -R s6log:s6log /var/log/lightdm }
envfile /etc/s6/config/lightdm.conf
importas -sCiu DIRECTIVES DIRECTIVES
s6-setuidgid s6log exec -c s6-log -d3 -b -- ${DIRECTIVES} /var/log/lightdm
