#!/usr/bin/execlineb -P
foreground { install -d -o s6log -g s6log /var/log/git-daemon }
envfile /etc/s6/config/git.conf
ifelse { test -w /var/log } {
foreground { s6-setuidgid s6log mkdir -p /var/log/git-daemon }
importas -sCiu DIRECTIVES DIRECTIVES
s6-setuidgid s6log exec -c s6-log -d3 -b -- ${DIRECTIVES} /var/log/git-daemon
}
foreground { install -d -o s6log -g s6log /run/log/git-daemon }
s6-setuidgid s6log exec -c s6-log -d3 -b -- ${DIRECTIVES} /run/log/git-daemon
