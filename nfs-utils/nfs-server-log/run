#!/usr/bin/execlineb -P
foreground { if -n -t { test -d /var/log/nfs-server } install -d -m 0755 -o s6log -g s6log /var/log/nfs-server }
envfile /etc/s6/config/nfs-utils.conf
importas -sCiu NFS_SERVER_DIRECTIVES NFS_SERVER_DIRECTIVES
s6-setuidgid s6log exec -c s6-log -d3 -b -- ${NFS_SERVER_DIRECTIVES} /var/log/nfs-server
