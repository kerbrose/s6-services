#!/usr/bin/execlineb -P
envfile /etc/s6/config/nfs-utils.conf
importas -sCiu STATD_DIRECTIVES STATD_DIRECTIVES
ifelse { test -w /var/log } {
foreground { if -n -t { test -d /var/log/statd } install -d -m 0755 -o s6log -g s6log /var/log/statd }
s6-setuidgid s6log exec -c s6-log -d3 -b -- ${STATD_DIRECTIVES} /var/log/statd
}
foreground { install -d -o s6log -g s6log /run/log/statd }
s6-setuidgid s6log exec -c s6-log -d3 -b -- ${DIRECTIVES} /run/log/statd
