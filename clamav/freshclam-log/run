#!/usr/bin/execlineb -P
foreground { if -n -t { test -d /var/log/freshclam } install -d -m 0755 -o s6log -g s6log /var/log/freshclam }
envfile /etc/s6/config/clamav.conf
importas -sCiu FRESHCLAM_DIRECTIVES FRESHCLAM_DIRECTIVES
s6-setuidgid s6log exec -c s6-log -d3 -b -- ${FRESHCLAM_DIRECTIVES} /var/log/freshclam
