#!/usr/bin/execlineb -P
foreground { if -n -t { test -d /var/log/lighttpd } install -d -m 0755 -o s6log -g s6log /var/log/lighttpd }
foreground { chmod -R 755 /var/log/lighttpd }
envfile /etc/s6/config/lighttpd.conf
importas -sCiu DIRECTIVES DIRECTIVES
s6-setuidgid s6log exec -c "s6-log -d3 -b -- ${DIRECTIVES} /var/log/lighttpd
