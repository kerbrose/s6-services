#!/usr/bin/execlineb -P
foreground { if -n -t { test -d /var/log/opensips } install -d -m 0755 -o s6log -g s6log /var/log/opensips }
foreground { chmod -R 755 /var/log/opensips }
envfile /etc/s6/config/opensips.conf
importas -sCiu DIRECTIVES DIRECTIVES
s6-setuidgid s6log exec -c s6-log -d3 -b -- ${DIRECTIVES} /var/log/opensips
