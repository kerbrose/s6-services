#!/usr/bin/execlineb -P
foreground { if -n -t { test -d /var/log/qemu-guest-agent } install -d -m 0755 -o s6log -g s6log /var/log/qemu-guest-agent }
envfile /etc/s6/config/qemu-guest-agent.conf
importas -sCiu DIRECTIVES DIRECTIVES
s6-setuidgid s6log exec -c s6-log -d3 -b -- ${DIRECTIVES} /var/log/qemu-guest-agent
