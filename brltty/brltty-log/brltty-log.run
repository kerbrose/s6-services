#!/usr/bin/execlineb -P
foreground { if -n -t { test -d /var/log/brltty } install -d -m 0755 -o s6log -g s6log /var/log/brltty }
foreground { chmod -R 755 /var/log/brltty }
envfile /etc/s6/sv/brltty-log/conf
importas -sCiu DIRECTIVES DIRECTIVES
s6-setuidgid s6log exec -c s6-log -d3 -b -- ${DIRECTIVES} /var/log/brltty
