#!/usr/bin/execlineb -P
foreground { if -n -t { test -d /var/log/lvmpolld } install -d -m 0755 -o s6log -g s6log /var/log/lvmpolld }
envfile /etc/s6/config/lvm2.conf
importas -sCiu LVMPOLLD_DIRECTIVES LVMPOLLD_DIRECTIVES
s6-setuidgid s6log exec -c s6-log -d3 -b -- ${LVMPOLLD_DIRECTIVES} /var/log/lvmpolld
